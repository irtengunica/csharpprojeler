<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<system.web>
		<customErrors mode="Off"></customErrors>
		<compilation debug="true"/>
		<authentication mode="Forms">
			<forms name="AuthForm" loginUrl="login.aspx"/>
		</authentication>
	</system.web>
	<location path="default.aspx">
		<system.web>
			<authorization>
				<deny users="?"/>
			</authorization>
		</system.web>
	</location>
</configuration>